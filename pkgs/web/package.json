{
  "name": "chessboard.pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "build": "next build",
